import pandas as pd
import numpy as np

def join_data(local):
    '''
    join_data Ã© para juntar bases
    local is a string - you define where are you data
    '''
    name = pd.DataFrame()
    for f in glob.glob(local+'*.xlsx'):
        df = pd.read_excel(f)
        name = name.append(df, ignore_index = True)
    return name.head()

